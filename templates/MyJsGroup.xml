<templateSet group="MyJsGroup">
  <template name="clog" value="console.log($END$);" description="Console log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="logi" value="Logger.info($END$);" description="Logger info" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
</templateSet>